function TargetNext()
{
	var serial = FindNearestTarget();
	if(!serial.length) return;
	Orion.CloseStatusbar('lasttarget');
	CharPrintDelayed(serial, '30',Orion.RequestName(serial,100)+ ' - TARGET');
