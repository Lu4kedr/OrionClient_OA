function SwitchArm(max)
{			
	for (var i = 0; i <= max; i++)
	{
		if(Orion.GetGlobal("weap")==i)
		{
			if((i+1)>max)
			{
				Orion.Arm(1);
				Orion.SetGlobal("weap",1);
			}
			else
			{
				Orion.Arm(i+1);
				Orion.SetGlobal("weap",i+1);
			}
		return;
		}
	}
}
