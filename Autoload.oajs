		Orion.Wait(10);
	}
	var start=Orion.Now();
	Orion.Attack(lasttarget);
	while(Player.Hidden())
	{
		Orion.Wait(10);
	}
	Orion.Print(Orion.Now()-start);
	Orion.WarMode(false);

}



