			Orion.Print("Do dalsiho lvl zbyva: "+xp2do+" zkusenosti.");
			//Orion.Print("ZabitÃ½ch "+Orion.GetGlobal('killCnt').toString()+" monster.");

			Orion.Print("Prumerne "+ prum + " expu za minutu.");
			Orion.Print("Celkovy cas: "+cas+" minut.");
